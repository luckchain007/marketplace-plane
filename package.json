{
  "repository": "https://github.com/makeplane/plane.git",
  "license": "AGPL-3.0",
  "private": true,
  "workspaces": [
    "web",
    "space",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
